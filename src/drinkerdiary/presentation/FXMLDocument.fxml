<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="drinkerdiary.presentation.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="14.0" layoutY="14.0" onAction="#handleButtonAction" text="Load All Drinker Names" />
      <ChoiceBox fx:id="drinkerNameList" layoutX="14.0" layoutY="61.0" prefHeight="25.0" prefWidth="141.0" />
      <Button fx:id="buttonNewDrinker" layoutX="171.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleNewDrinkerButton" text="New Drinker" />
      <Button fx:id="newSubstanceButton" layoutX="171.0" layoutY="61.0" mnemonicParsing="false" onAction="#handleNewSubstanceButton" text="New Substance" />
    </children>
</AnchorPane>
